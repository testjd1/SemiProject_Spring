 <select id="threv" parameterType="ReservationVO" resultType="hashmap">

 select r.themename as themename, r.branch as branch, t.thcost as thcost
 from reservetheme r full join theme t
 on r.picture = t.picture
 where  r.picture = #{picture}
		

</select> 
 

    
